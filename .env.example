# Razorpay Configuration (Required for Payment Processing)
# Get these keys from your Razorpay dashboard: https://dashboard.razorpay.com/
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here

# Firebase Configuration (Required for Visitor Analytics)
# Get this JSON config from Firebase Console and stringify it
VITE_FIREBASE_CONFIG={"apiKey":"your-api-key","authDomain":"your-domain","projectId":"your-project-id","storageBucket":"your-bucket","messagingSenderId":"123456789","appId":"your-app-id"}

# Gemini AI Configuration (Required for AI Features)
# (Removed) Google AI API key no longer used.
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Example values (DO NOT USE IN PRODUCTION):
# VITE_RAZORPAY_KEY_ID=rzp_test_1DP5mmOlF5G5ag
# VITE_GEMINI_API_KEY=AIzaSyA-example-key-do-not-use